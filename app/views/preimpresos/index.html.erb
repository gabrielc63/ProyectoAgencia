<section id="main-content">

  <section class="wrapper site-min-height">
    <!-- /.col-lg-12 -->
    <div class="row mt">
      <div class="col-lg-12">
        <div class="content-panel">
          <h4>Listado de juegos preimpresos</h4>
            <section id="unseen">
              <table class="table table-striped table-bordered table-condensed">
                <thead>
                  <tr>
                    <th>Fecha sorteo</th>
                    <th>Fecha inicio</th>
                    <th>Numero sorteo</th>
                    <th>Precio</th>
                    <th>Cantidad</th>
                    <th>Tipo</th>
                    <th>Devolucion</th>
                    <th>Agencia</th>
                    <th colspan="3"></th>
                  </tr>
                </thead>

                <tbody>
                  <% @preimpresos.each do |preimpreso| %>
                    <tr>
                      <td><%= preimpreso.fecha_sorteo %></td>
                      <td><%= preimpreso.fecha_inicio %></td>
                      <td><%= preimpreso.numero_sorteo %></td>
                      <td><%= preimpreso.precio %></td>
                      <td><%= preimpreso.cantidad %></td>
                      <td><%= preimpreso.tipo %></td>
                      <td><%= preimpreso.devolucion %></td>
                      <td><%= preimpreso.agencia %></td>
                      <td><%= link_to 'Show', preimpreso %></td>
                      <td><%= link_to 'Edit', edit_preimpreso_path(preimpreso) %></td>
                      <td><%= link_to 'Destroy', preimpreso, method: :delete, data: { confirm: 'Are you sure?' } %></td>
                    </tr>
                  <% end %>
                </tbody>
              </table>
<%= link_to 'New Preimpreso', new_preimpreso_path %>
            </section>
          </div>
        </div>
      </div>
    </section>

  </section>
