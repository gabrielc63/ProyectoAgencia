<% provide(:title, "Listado Subagencias") %>
<div id="wrapper">
  <%= render 'agencias/navigation' %>
  <div id="page-wrapper">
    <div class="row">
      <div class="col-lg-12">
        <h1 class="page-header">Subagencias</h1>
      </div>
      <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
    <div class="row">
      <div class="col-lg-12">
        <div class="panel panel-default">
          <div class="panel-heading">Listado de subagencias</div>
          <div class="panel-body">
            <div class="table-responsive">
              <table class="table table-striped table-bordered table-hover">
                <thead>
                  <tr>
                    <th>#</th>
                    <th>Titular</th>
                    <th>Domicilio</th>
                    <th>Elegir</th>
                  </tr>
                </thead>
                <tbody>
                  <% @subagencias.each do |s|%>
                    <tr>
                      <td><%= s.numero_subagencia %></td>
                      <td><%= s.titular %></td>
                      <td><%= s.domicilio %></td>
                      <td><a href="<%= subagencia_path s %>">Ir</a></td>
                    </tr>
                  <% end %>
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!--/ .row -->
  </div>
  <!-- /#page-wrapper -->
</div>
