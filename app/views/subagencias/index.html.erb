<% provide(:title, "Listado Subagencias") %>

  <section id="main-content">

    <section class="wrapper site-min-height">
        <h3><i class="fa fa-angle-right"></i>Listado de subagencias</h3>
      <!-- /.col-lg-12 -->
      <div class="row mt">
        <div class="col-lg-12">
          <div class="content-panel">
            <h4>Listado de subagencias</h4>
              <section id="unseen">
              <table class="table table-striped table-bordered table-condensed">
                <thead>
                  <tr>
                    <th>#</th>
                    <th>Titular</th>
                    <th>Domicilio</th>
                    <th>Ver</th>
                    <th>Modificar</th>
                  </tr>
                </thead>
                <tbody>
                  <% @subagencias.each do |s|%>
                    <tr>
                      <td><%= s.numero_subagencia %></td>
                      <td><%= s.titular %></td>
                      <td><%= s.domicilio %></td>
                      <td><a href="<%= subagencia_path(s) %>">Ir</a></td>
                      <td><%= link_to("Editar", edit_subagencia_path(s)) %>
                      </td>
                    </tr>
                  <% end %>
                </tbody>
              </table>
            </section>
          </div>
        </div>
      </div>
    </section>

  </section>
