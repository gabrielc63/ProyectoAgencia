<section id="main-content">
  <section class="wrapper">

      <div class="row">
        <div class="col-md-6 col-md-offset-3">
          <h2>Nueva Subagencia</h2>

          <div class="form-panel">
            <%= form_for @subagencia, :url => subagencias_path do |f| %>
              <%= render 'shared/error_messages', object: f.object %>

                <div class="form-group">
                  <%= f.label :titular %>
                  <%= f.text_field :titular, class: "form-control" %>
                </div>

                <div class="form-group">
                  <%= f.label :numero_subagencia %>
                  <%= f.text_field :numero_subagencia, class: "form-control" %>
                </div>

                <div class="form-group">
                  <%= f.label :email %>
                  <%= f.text_field :email, class: "form-control" %>
                </div>

                <div class="form-group">
                  <%= f.label :telefono %>
                  <%= f.text_field :telefono, class: "form-control" %>
                </div>

                <div class="form-group">
                  <%= f.label :domicilio %>
                  <%= f.text_field :domicilio, class: "form-control" %>
                </div>

                <%= f.submit "Agregar subagencia", class: "btn btn-default btn-lg btn-primary" %>
              <% end %>
          </div>

        </div>
      </div>

  </section>
</section>
