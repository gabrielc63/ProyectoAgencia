<%= render 'agencias/navigation' %>
<div id="page-wrapper">
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <h2>Nuevo juego impreso</h2>

    <%= form_for [@subagencia, @juego_impreso] do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
      <%= f.hidden_field :subagencia_id , :value=>params[:subagencia_id] %>

      <div class="form-group">
        <%= f.label :quiniela %>
        <%= f.text_field :quiniela, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= f.label :quini_express %>
        <%= f.text_field :quini_ex, class: "form-control" %>
      </div>

      <div class="form-group">
      <%= f.label :loteria_online %>
      <%= f.text_field :loteria_online, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= f.label :otros_juegos %>
        <%= f.text_field :otros_juegos, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= f.label :otros_gastos %>
        <%= f.text_field :otros_gastos, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= f.label :premios %>
        <%= f.text_field :premios, class: "form-control" %>
      </div>

      <%= f.submit "Guardar", class: "btn btn-default btn-lg btn-primary" %>
    <% end %>
  </div>
</div>
</div>
