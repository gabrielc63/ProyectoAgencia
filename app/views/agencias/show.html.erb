<% provide(:title, "Agencia #{@agencia.numero_agencia}") %>

  <section id="main-content">
    <section class="wrapper site-min-height">
      <div class="row">
        <div class="col-lg-12">
          <h1 class="page-header">
            Agencia
            <%= @agencia.numero_agencia %>
          </h1>
          <p>Titular:
            <%= @agencia.titular %></p>
          <p>Telefono:
            <%= @agencia.telefono %></p>
          <p>Domicilio:
            <%= @agencia.domicilio %></p>
        </div>
      </div>
    </section>
  </section>
